# ADK Interactions Agent Environment Configuration
# Copy this file to .env and fill in your values

# Google API Key (Required)
# Get your key from: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# Optional: Gemini Model Selection
# Options: gemini-2.5-flash, gemini-2.5-pro, gemini-2.5-flash-lite
# GEMINI_MODEL=gemini-2.5-flash

# Optional: Enable Interactions API (default: true)
# Set to "false" to use standard API
# USE_INTERACTIONS_API=true

# Optional: Enable debug logging
# DEBUG=false
